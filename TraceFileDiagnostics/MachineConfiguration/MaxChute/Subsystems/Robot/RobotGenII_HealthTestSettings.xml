<RobotGenII_HealthTestSettings>
  <RobotXMotorHealthTestProcess>
    <Timeout>45000</Timeout>
    <SecondMoveTarget>0.5</SecondMoveTarget>
    <TrajectoryControlParametersName>Creep</TrajectoryControlParametersName>
    <MaxHealthyAverageCurrent>2.00</MaxHealthyAverageCurrent>
    <MaxHealthyPositionError>0.160</MaxHealthyPositionError>
  </RobotXMotorHealthTestProcess>
  <RobotZMotorHealthTestProcess>
    <Timeout>45000</Timeout>
    <TrajectoryControlParametersName>Creep</TrajectoryControlParametersName>
    <MaxHealthyAverageCurrent>3.20</MaxHealthyAverageCurrent>
    <MaxHealthyPositionError>0.100</MaxHealthyPositionError>
  </RobotZMotorHealthTestProcess>
  <RobotThetaMotorHealthTestProcess>
    <Timeout>30000</Timeout>
    <TurnAroundVelocityScalingFactor>0.180</TurnAroundVelocityScalingFactor>
    <TurnAroundAccelerationScalingFactor>0.040</TurnAroundAccelerationScalingFactor>
    <MaxHealthyAverageCurrent>1.50</MaxHealthyAverageCurrent>
    <MaxHealthyPositionError>0.650</MaxHealthyPositionError>
  </RobotThetaMotorHealthTestProcess>
  <RobotGripperMotorHealthTestProcess>
    <Timeout>30000</Timeout>
    <FirstMoveTarget>1.20</FirstMoveTarget>
    <SecondMoveTarget>0.05</SecondMoveTarget>
    <TrajectoryControlParametersName>LightTouch</TrajectoryControlParametersName>
    <MaxHealthyAverageCurrent>0.2675</MaxHealthyAverageCurrent>
    <MaxHealthyPositionError>0.160</MaxHealthyPositionError>
  </RobotGripperMotorHealthTestProcess>
  <RobotHealthTestProcess>
    <Timeout>150000</Timeout>
    <ControlsProcessType>Diagnostic</ControlsProcessType>
  </RobotHealthTestProcess>
  <RobotStartUpProcess>
    <Timeout>0</Timeout>
  </RobotStartUpProcess>
  <RobotNumericIOHealthTestProcess>
    <Timeout>0</Timeout>
    <TestingThresholds>
      <IOName>RobotThetaPositionErrorInput</IOName>
      <ActiveThreshold>Infinity</ActiveThreshold>
      <InactiveThreshold>-Infinity</InactiveThreshold>
    </TestingThresholds>
    <TestingThresholds>
      <IOName>RobotXPositionErrorInput</IOName>
      <ActiveThreshold>Infinity</ActiveThreshold>
      <InactiveThreshold>-Infinity</InactiveThreshold>
    </TestingThresholds>
  </RobotNumericIOHealthTestProcess>
</RobotGenII_HealthTestSettings>
