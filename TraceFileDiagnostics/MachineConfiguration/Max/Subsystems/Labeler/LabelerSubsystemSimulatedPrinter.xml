<LabelerSubsystem>
   <Filename>LabelerSimulatedPrinter.xml</Filename>
   <Filename>LabelerDevices.xml</Filename>
   <Labeler>
      <Type assembly="_MaxAssembly_" namespace="_MaxNamespace_">Labeler</Type>
      <ConfigurationParameters>
         <Subsystem>
            <VialFeeder>
               <Name>LeftVialFeeder</Name>
               <Location>27</Location>
            </VialFeeder>
            <VialFeeder>
               <Name>RightVialFeeder</Name>
               <Location>137</Location>
            </VialFeeder>
            <Cup>
               <Name>LeftCup</Name>
               <Location>27</Location>
               <Diameter>2.000</Diameter>
            </Cup>
            <Cup>
               <Name>RightCup</Name>
               <Location>137</Location>
               <Diameter>1.500</Diameter>
            </Cup>
            <DriveRollerToPrinterSpeedRatio>0.95</DriveRollerToPrinterSpeedRatio>
         </Subsystem>
         <LabelerHomingProcess>
            <Timeout>30000</Timeout>
            <Offset>-17</Offset>
         </LabelerHomingProcess>
         <LabelerAlignCupProcess>
            <Timeout>30000</Timeout>
         </LabelerAlignCupProcess>
         <LabelerMoveToPrinterProcess>
            <Timeout>30000</Timeout>
            <VialSettleTime>2000</VialSettleTime>
            <PrinterLocation>186</PrinterLocation>
         </LabelerMoveToPrinterProcess>
         <LabelerPrintLabelProcess>
            <Timeout>30000</Timeout>
            <PrinterSpeed>82</PrinterSpeed>
            <SwingArmDisengageTime>500</SwingArmDisengageTime>
         </LabelerPrintLabelProcess>
         <LabelerDeliverVialProcess>
            <Timeout>30000</Timeout>
            <VialMissingPercentage>75</VialMissingPercentage>
            <VialLabeledPercentage>30</VialLabeledPercentage>
         </LabelerDeliverVialProcess>
         <LabelerLabelVialProcess>
            <Timeout>0</Timeout>
            <FeedVialTimeout>5000</FeedVialTimeout>
            <VialFeedSettleTime>0</VialFeedSettleTime>
         </LabelerLabelVialProcess>
      </ConfigurationParameters>
   </Labeler>
</LabelerSubsystem>
