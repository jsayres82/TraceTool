<MaxSystemManager>
  <Max>
    <Type assembly="_MaxAssembly_" namespace="_MaxNamespace_">MaxSystemManager</Type>
    <ConfigurationParameters>
      <Subsystem>
        <IsSimulation>False</IsSimulation>
        <ShouldLogAllMacroStates>False</ShouldLogAllMacroStates>
      </Subsystem>
      <MaxHomeProcess>
        <Timeout>120000</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </MaxHomeProcess>
      <MaxParkProcess>
        <Timeout>60000</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </MaxParkProcess>
      <MaxCalibrateProcess>
        <Timeout>-1</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <Filename>MaxCalibration__CellType_.xml</Filename>
        <Calibration>
          <Calibratable>Capper</Calibratable>
          <ErrorCode>
            <ErrorName>CapperCalibrateError</ErrorName>
            <ErrorType assembly="Parata.Controls.Enumerations.Capper" namespace="Parata.Controls.Enumerations.ErrorCodes">CapperErrorCode</ErrorType>
          </ErrorCode>
        </Calibration>
        <Calibration>
          <Calibratable>Robot</Calibratable>
          <ErrorCode>
            <ErrorName>RobotCalibrationError</ErrorName>
            <ErrorType assembly="Parata.Controls.Enumerations.Robot" namespace="Parata.Controls.Enumerations.ErrorCodes">RobotErrorCode</ErrorType>
          </ErrorCode>
        </Calibration>
      </MaxCalibrateProcess>
      <MaxFillProcess>
        <Timeout>0</Timeout>
        <IdleTimeToTurnOffEPR>900000</IdleTimeToTurnOffEPR>
        <MaximumRecentFills>5</MaximumRecentFills>
        <ShouldTestForObstruction>False</ShouldTestForObstruction>
      </MaxFillProcess>
      <BreakawayFillSelectProcess>
        <Timeout>0</Timeout>
        <PollPeriod>1000</PollPeriod>
      </BreakawayFillSelectProcess>
      <BreakawayAddFillRequestProcess>
        <Timeout>0</Timeout>
        <NumberOfFillsToAdd>1</NumberOfFillsToAdd>
        <Filename>FillRequestList.xml</Filename>
      </BreakawayAddFillRequestProcess>
      <BreakawaySetUpTestFillsProcess>
        <Timeout>0</Timeout>
      </BreakawaySetUpTestFillsProcess>
      <BreakawayRemoveAllFillRequestsProcess>
        <Timeout>30</Timeout>
      </BreakawayRemoveAllFillRequestsProcess>
      <MaxDropOffProcess>
        <Timeout>0</Timeout>
      </MaxDropOffProcess>
      <MaxTestDropOffProcess>
        <Timeout>0</Timeout>
      </MaxTestDropOffProcess>
      <MaxHealthTestProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <HealthTestFrequency>86400000</HealthTestFrequency>
        <ShouldRunOnStartUp>True</ShouldRunOnStartUp>
      </MaxHealthTestProcess>
      <MaxRecoverRobotProcess>
        <Timeout>0</Timeout>
      </MaxRecoverRobotProcess>
      <MaxStartUpProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </MaxStartUpProcess>
      <MaxSubsystemsStartUpProcess>
        <Timeout>0</Timeout>
      </MaxSubsystemsStartUpProcess>
      <MaxNumericIOHealthTestProcess>
        <Timeout>0</Timeout>
      </MaxNumericIOHealthTestProcess>
      <BreakawayUpdateFirmwareProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </BreakawayUpdateFirmwareProcess>
      <SystemCauseTimeGapProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <SpinTime>6100</SpinTime>
        <ShouldSuspend>True</ShouldSuspend>
      </SystemCauseTimeGapProcess>
      <SystemExportPerformanceDataProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <ExportFileName>PerformanceData.csv</ExportFileName>
      </SystemExportPerformanceDataProcess>
      <BreakawaySetUpFillingFromAllCellsProcess>
        <Timeout>30000</Timeout>
      </BreakawaySetUpFillingFromAllCellsProcess>
      <MaxRetrieveVialFromLabelerProcess>
        <Timeout>0</Timeout>
        <DropOff>C1</DropOff>
      </MaxRetrieveVialFromLabelerProcess>
    </ConfigurationParameters>
  </Max>
</MaxSystemManager>
