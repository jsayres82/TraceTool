<ExpressPlusSystemManager>
   <Filename>ExpressPlusSystemDevices.xml</Filename>
   <Express>
      <Type assembly="_ExpressPlusAssembly_" namespace="_ExpressPlusNamespace_">ExpressPlusSystemManager</Type>
      <ConfigurationParameters>
         <Subsystem>
            <IsSimulation>False</IsSimulation>
            <ShouldLogAllMacroStates>False</ShouldLogAllMacroStates>
         </Subsystem>
         <ExpressHomeProcess>
            <Timeout>120000</Timeout>
            <ControlsProcessType>Diagnostic</ControlsProcessType>
         </ExpressHomeProcess>
         <ExpressParkProcess>
            <Timeout>60000</Timeout>
            <ControlsProcessType>Diagnostic</ControlsProcessType>
         </ExpressParkProcess>
         <ExpressCalibrateProcess>
            <Timeout>-1</Timeout>
            <ControlsProcessType>Diagnostic</ControlsProcessType>
         </ExpressCalibrateProcess>
         <ExpressPlusFillProcess>
            <Timeout>0</Timeout>
            <IdleTimeToTurnOffEPR>900000</IdleTimeToTurnOffEPR>
            <MaximumRecentFills>5</MaximumRecentFills>
         </ExpressPlusFillProcess>
         <BreakawayFillSelectProcess>
            <Timeout>0</Timeout>
            <PollPeriod>1000</PollPeriod>
         </BreakawayFillSelectProcess>
         <BreakawayAddFillRequestProcess>
            <Timeout>0</Timeout>
            <NumberOfFillsToAdd>1</NumberOfFillsToAdd>
            <Filename>FillRequestList.xml</Filename>
            <ControlsProcessType>Diagnostic</ControlsProcessType>
         </BreakawayAddFillRequestProcess>
         <BreakawaySetUpTestFillsProcess>
           <Timeout>0</Timeout>
         </BreakawaySetUpTestFillsProcess>
         <BreakawayRemoveAllFillRequestsProcess>
            <Timeout>30</Timeout>
            <ControlsProcessType>Diagnostic</ControlsProcessType>
         </BreakawayRemoveAllFillRequestsProcess>
         <ExpressDropOffProcess>
            <Timeout>0</Timeout>
         </ExpressDropOffProcess>
         <ExpressTestDropOffProcess>
            <Timeout>0</Timeout>
         </ExpressTestDropOffProcess>
         <ExpressHealthTestProcess>
            <Timeout>0</Timeout>
            <ControlsProcessType>Diagnostic</ControlsProcessType>
            <HealthTestFrequency>86400000</HealthTestFrequency>
            <ShouldRunOnStartUp>True</ShouldRunOnStartUp>
         </ExpressHealthTestProcess>
         <ExpressRecoverRobotProcess>
            <Timeout>0</Timeout>
         </ExpressRecoverRobotProcess>
         <ExpressStartUpProcess>
            <Timeout>0</Timeout>
            <ControlsProcessType>Diagnostic</ControlsProcessType>
         </ExpressStartUpProcess>
         <ExpressSubsystemsStartUpProcess>
            <Timeout>0</Timeout>
         </ExpressSubsystemsStartUpProcess>
         <ExpressNumericIOHealthTestProcess>
            <Timeout>0</Timeout>
         </ExpressNumericIOHealthTestProcess>
         <BreakawayUpdateFirmwareProcess>
