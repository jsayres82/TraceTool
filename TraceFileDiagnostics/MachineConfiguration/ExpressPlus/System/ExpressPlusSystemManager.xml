               <Parity>None</Parity>
               <StopBits>1</StopBits>
               <ReadToByte>13</ReadToByte>
               <Encoding>IBM437</Encoding>
               <ReadTimeout>1000</ReadTimeout>
            </P1Reader>
            <P2Reader>
              <PortNameList>
