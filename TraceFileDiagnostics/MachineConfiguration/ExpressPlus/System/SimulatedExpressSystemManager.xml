<SimulatedExpressSystemManager>
  <Express>
    <Type assembly="_ExpressAssembly_" namespace="_ExpressNamespace_">ExpressSystemManager</Type>
    <ConfigurationParameters>
      <Subsystem>
        <IsSimulation>True</IsSimulation>
        <ShouldLogAllMacroStates>False</ShouldLogAllMacroStates>
      </Subsystem>
      <ExpressHomeProcess>
        <Timeout>60000</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </ExpressHomeProcess>
      <ExpressParkProcess>
        <Timeout>60000</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </ExpressParkProcess>
      <ExpressCalibrateProcess>
        <Timeout>-1</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </ExpressCalibrateProcess>
      <ExpressFillProcess>
        <Timeout>0</Timeout>
        <SimulationSequenceFilename>SimulatedSystemFillSequence.xml</SimulationSequenceFilename>
        <MaximumRecentFills>5</MaximumRecentFills>
      </ExpressFillProcess>
      <BreakawayFillSelectProcess>
        <Timeout>0</Timeout>
        <PollPeriod>1000</PollPeriod>
      </BreakawayFillSelectProcess>
      <BreakawayAddFillRequestProcess>
        <Timeout>0</Timeout>
        <NumberOfFillsToAdd>1</NumberOfFillsToAdd>
        <Filename>FillRequestList.xml</Filename>
      </BreakawayAddFillRequestProcess>
      <BreakawaySetUpTestFillsProcess>
        <Timeout>0</Timeout>
      </BreakawaySetUpTestFillsProcess>
      <BreakawayRemoveAllFillRequestsProcess>
        <Timeout>30</Timeout>
      </BreakawayRemoveAllFillRequestsProcess>
      <ExpressDropOffProcess>
        <Timeout>0</Timeout>
      </ExpressDropOffProcess>
      <ExpressTestDropOffProcess>
        <Timeout>0</Timeout>
      </ExpressTestDropOffProcess>
      <ExpressHealthTestProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <HealthTestFrequency>86400000</HealthTestFrequency>
        <ShouldRunOnStartUp>True</ShouldRunOnStartUp>
      </ExpressHealthTestProcess>
      <ExpressRecoverRobotProcess>
        <Timeout>0</Timeout>
      </ExpressRecoverRobotProcess>
      <ExpressStartUpProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </ExpressStartUpProcess>
      <ExpressSubsystemsStartUpProcess>
        <Timeout>0</Timeout>
      </ExpressSubsystemsStartUpProcess>
      <ExpressNumericIOHealthTestProcess>
        <Timeout>0</Timeout>
      </ExpressNumericIOHealthTestProcess>
      <BreakawayUpdateFirmwareProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </BreakawayUpdateFirmwareProcess>
      <SystemCauseTimeGapProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <SpinTime>6100</SpinTime>
        <ShouldSuspend>True</ShouldSuspend>
      </SystemCauseTimeGapProcess>
      <SystemExportPerformanceDataProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <ExportFileName>PerformanceData.csv</ExportFileName>
      </SystemExportPerformanceDataProcess>
      <ExpressAutomatedFillTestProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <P1Reader>
          <PortName>COM21</PortName>
          <BaudRate>9600</BaudRate>
          <DataBits>8</DataBits>
          <Parity>None</Parity>
          <StopBits>1</StopBits>
          <ReadToByte>13</ReadToByte>
          <Encoding>IBM437</Encoding>
          <ReadTimeout>1000</ReadTimeout>
        </P1Reader>
        <P2Reader>
          <PortName>COM22</PortName>
          <BaudRate>9600</BaudRate>
          <DataBits>8</DataBits>
          <Parity>None</Parity>
          <StopBits>1</StopBits>
          <ReadToByte>13</ReadToByte>
          <Encoding>IBM437</Encoding>
          <ReadTimeout>1000</ReadTimeout>
        </P2Reader>
      </ExpressAutomatedFillTestProcess>
      <BreakawaySetUpFillingFromAllCellsProcess>
        <Timeout>30000</Timeout>
      </BreakawaySetUpFillingFromAllCellsProcess>
    </ConfigurationParameters>
  </Express>
</SimulatedExpressSystemManager>
