<RightCapFeederSubsystem>
  <Filename>RightCapFeederDevices.xml</Filename>
  <RightCapFeeder>
    <Type assembly="_MaxAssembly_" namespace="_MaxNamespace_">CapFeeder</Type>
    <ConfigurationParameters>
      <Subsystem>
        <CapType>_RightVialType_</CapType>
      </Subsystem>
      <RightCapFeederStageProcess>
        <Timeout>600000</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <InterlockDebounceTime>500</InterlockDebounceTime>
        <MotorErrorDebounceTime>100</MotorErrorDebounceTime>
        <InterlockOpenTimeout>60000</InterlockOpenTimeout>
        <InterlockOpenWarningsBeforeError>4</InterlockOpenWarningsBeforeError>
        <StagingTimeout>120000</StagingTimeout>
        <StagingJamLimit>25</StagingJamLimit>
        <StageSensorPollTime>5000</StageSensorPollTime>
      </RightCapFeederStageProcess>
      <RightCapFeederFeedProcess>
        <Timeout>600000</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
        <InterlockDebounceTime>500</InterlockDebounceTime>
        <MotorErrorDebounceTime>100</MotorErrorDebounceTime>
        <InterlockOpenTimeout>60000</InterlockOpenTimeout>
        <InterlockOpenWarningsBeforeError>4</InterlockOpenWarningsBeforeError>
        <FeedTimeout>120000</FeedTimeout>
        <FeedJamLimit>25</FeedJamLimit>
        <FeedDistance>
          <KERR13>62.5</KERR13>
          <KERR16>62.5</KERR16>
          <KERR20>62.5</KERR20>
          <KERR30>58.6</KERR30>
          <KERR40>58.6</KERR40>
          <PT13>77.4</PT13>
          <PT16>77.4</PT16>
          <PT20>77.4</PT20>
          <PT30>77.4</PT30>
          <PT40>77.4</PT40>
          <TriState_PRX13>77.4</TriState_PRX13>
          <TriState_PRX16>77.4</TriState_PRX16>
          <TriState_PRX30>77.4</TriState_PRX30>
          <TriState_PRX40>77.4</TriState_PRX40>
          <RC13>69.0</RC13>
          <RC16>69.0</RC16>
          <RC20>69.0</RC20>
          <RC30>71.2</RC30>
          <RC40>71.2</RC40>
        </FeedDistance>
      </RightCapFeederFeedProcess>
      <RightCapFeederMotorHealthTestProcess>
        <Timeout>30000</Timeout>
        <!--These targets are velocity targets.-->
        <FirstMoveTarget>130e-3</FirstMoveTarget>
        <SecondMoveTarget>-130e-3</SecondMoveTarget>
        <FirstMoveDuration>10000</FirstMoveDuration>
        <SecondMoveDuration>10000</SecondMoveDuration>
        <MinForceLimit Units="Amps">-1.489</MinForceLimit>
        <MaxForceLimit Units="Amps">1.489</MaxForceLimit>
      </RightCapFeederMotorHealthTestProcess>
      <RightCapFeederRecoveryProcess>
        <Timeout>600000</Timeout>
        <InterlockDebounceTime>500</InterlockDebounceTime>
        <MotorErrorDebounceTime>100</MotorErrorDebounceTime>
        <InterlockOpenTimeout>60000</InterlockOpenTimeout>
        <InterlockOpenWarningsBeforeError>4</InterlockOpenWarningsBeforeError>
        <ReverseMoveDistance>35.0</ReverseMoveDistance>
        <ForwardMoveDistance>35.0</ForwardMoveDistance>
      </RightCapFeederRecoveryProcess>
      <RightCapFeederNumericIOHealthTestProcess>
        <Timeout>0</Timeout>
      </RightCapFeederNumericIOHealthTestProcess>
      <RightCapFeederHealthTestProcess>
        <Timeout>0</Timeout>
        <ControlsProcessType>Diagnostic</ControlsProcessType>
      </RightCapFeederHealthTestProcess>
    </ConfigurationParameters>
  </RightCapFeeder>
</RightCapFeederSubsystem>