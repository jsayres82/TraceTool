<ExpcaSystemManager>
  <Filename>ExpcaSystemDevices.xml</Filename>
  <ExpressCA>
    <Type assembly="_ExpressCAAssembly_" namespace="_ExpressCANamespace_">ExpcaSystemManager</Type>
